<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="Console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d [%t] %-5p %-30.30c -%m%n</pattern>
    </encoder>
  </appender>
  <logger name="org.springside.modules" level="INFO"/>
  <root level="WARN">
    <appender-ref ref="Console"/>
  </root>
</configuration>